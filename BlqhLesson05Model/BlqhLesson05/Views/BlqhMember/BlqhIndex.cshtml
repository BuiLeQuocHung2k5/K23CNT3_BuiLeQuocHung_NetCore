@model IEnumerable<BlqhLesson05.Models.DataModels.BlqhMember>
@{
    ViewData["Title"] = "BlqhIndex";
}

<h1> Danh Sach member</h1>
<table class=" table table-bordered table-hover">
    <thead>
        <tr>
            <th>Member ID</th>
            <th>Member UserName</th>
            <th>Member PassWord</th>
            <th>Member Fullname</th>
            <th>Member Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var blqhItem in Model)
        {
            <tr>
                <td>@blqhItem.BlqhMemberId</td>
                <td>@blqhItem.BlqhUserName</td>
                <td>@blqhItem.BlqhPassword</td>
                <td>@blqhItem.BlqhFullname</td>
                <td>@blqhItem.BlqhEmail</td>
            </tr>
        }
    </tbody>
</table>

